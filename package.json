{
  "name": "application",
  "version": "0.0.0",
  "description": "Quickstart",
  "license": "MIT",
  "scripts": {
    "prestart": "yarn build:server",
    "start": "concurrently --kill-others-on-fail \"yarn start:server\" \"yarn start:client\"",
    "start:server": "node dist/server/server.js",
    "start:client": "ng serve --no-progress",
    "prestart:prod": "yarn build",
    "start:prod": "node dist/server/server.js",
    "ng": "ng",
    "build": "concurrently --kill-others-on-fail \"yarn build:server\" \"yarn build:client\"",
    "build:server": "tsc -p ./server",
    "build:client": "ng build --prod --no-progress",
    "test": "concurrently --kill-others-on-fail \"yarn test:server\" \"yarn test:client\"",
    "test:server": "echo \"no server test\"",
    "test:client": "ng test -cc --single-run --no-progress",
    "e2e": "ng e2e",
    "postinstall": "npm rebuild node-sass",
    "commitmsg": "validate-commit-msg",
    "lint": "concurrently --kill-others-on-fail \"yarn lint:server\" \"yarn lint:client\"",
    "lint:server": "tslint --type-check --project ./server/tsconfig.json -c ./tslint.json './server/**/*.ts'",
    "lint:client": "ng lint --type-check",
    "tslint": "tslint"
  },
  "private": true,
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "ci",
        "build"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": ""
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@angular/animations": "4.1.3",
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@angular/router": "4.1.3",
    "@nestjs/common": "*",
    "@nestjs/core": "*",
    "@nestjs/microservices": "*",
    "@nestjs/testing": "*",
    "@nestjs/websockets": "*",
    "body-parser": "^1.17.2",
    "core-js": "2.4.1",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "hammerjs": "2.0.8",
    "leaflet": "1.0.3",
    "morgan": "^1.8.2",
    "redis": "2.7.1",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.1.0",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.1.1",
    "@angular/compiler-cli": "4.1.3",
    "@angular/language-service": "4.0.0",
    "@types/body-parser": "^1.16.4",
    "@types/core-js": "^0.9.42",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.36",
    "@types/hammerjs": "2.0.34",
    "@types/jasmine": "2.5.45",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.0.7",
    "chart.js": "2.6.0",
    "codelyzer": "3.0.1",
    "concurrently": "^3.5.0",
    "conventional-changelog": "1.1.3",
    "husky": "0.13.4",
    "jasmine-core": "2.6.2",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.1.2",
    "ts-node": "3.0.4",
    "tslint": "5.4.3",
    "tslint-config-standard": "5.0.2",
    "typescript": "2.3.3",
    "validate-commit-msg": "2.12.2"
  }
}
